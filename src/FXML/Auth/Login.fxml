<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="585.0" prefWidth="474.0" styleClass="background-transparent" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Auth.LoginController">
   <children>
      <Label layoutX="196.0" layoutY="14.0" styleClass="form-right-title" text="Login">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="166.0" styleClass="form-username-label" text="Username" />
      <TextField fx:id="usernameField" layoutX="40.0" layoutY="206.0" prefHeight="33.0" prefWidth="404.0" promptText="Enter username" styleClass="text-field" />
      <Label layoutX="35.0" layoutY="265.0" styleClass="form-password-label" text="Password" />
      <Label layoutX="39.0" layoutY="459.0" text="Don't have an account?" />
      <Button fx:id="loginBtn" layoutX="40.0" layoutY="508.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="404.0" styleClass="btn" text="Login" />
      <PasswordField fx:id="passwordField" layoutX="39.0" layoutY="299.0" prefHeight="33.0" prefWidth="404.0" promptText="Enter password" />
      <Label fx:id="signupLink" layoutX="202.0" layoutY="459.0" styleClass="signup-link" text="Signup now" />
      <Label fx:id="errorMessage" layoutX="40.0" layoutY="385.0" text="Error" textFill="RED" visible="false" />
   </children>
   <stylesheets>
      <URL value="@../../stylesheets/Application.css" />
      <URL value="@../../stylesheets/Auth.css" />
   </stylesheets>
</AnchorPane>
