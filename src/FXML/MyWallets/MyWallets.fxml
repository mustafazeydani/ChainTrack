<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="MyWallets" prefHeight="739.0" prefWidth="1027.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MyWallets.MyWalletsController">
   <children>
      <AnchorPane prefHeight="62.0" prefWidth="1027.0">
         <children>
            <Button layoutX="14.0" layoutY="16.0" mnemonicParsing="false" onAction="#handleAddWallet" prefHeight="33.0" prefWidth="187.0" styleClass="btn" text="Add New Wallet" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="64.0" prefWidth="1027.0">
         <children>
            <ComboBox fx:id="walletsComboBox" layoutX="14.0" layoutY="14.0" prefHeight="33.0" prefWidth="368.0" promptText="Select a wallet" />
         </children>
      </AnchorPane>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <content>
            <GridPane fx:id="tokensGrid" prefHeight="7.0" prefWidth="0.0">
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </content>
      </ScrollPane>
   </children>
   <stylesheets>
      <URL value="@../../stylesheets/MyWallets.css" />
   </stylesheets>
</VBox>
