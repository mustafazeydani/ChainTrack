         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" />
         </padding>
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ScrollPane prefHeight="300.0" prefWidth="1027.0">
         <content>
            <HBox fx:id="NFTCardsLayout" prefWidth="1024.0" />
         </content>
      </ScrollPane>
      <HBox prefHeight="30.0" prefWidth="1027.0">
         <children>
            <Label prefHeight="40.0" prefWidth="698.0" text="Cryptocurrencies">
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" />
               </padding>
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField id="search-bar" fx:id="searchInput" onKeyReleased="#onSearch" prefHeight="35.0" prefWidth="320.0" promptText="Search" />
         </children>
      </HBox>
      <Pagination fx:id="pagination" prefHeight="429.0" prefWidth="1027.0" />
